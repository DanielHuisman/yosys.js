{
    "name": "yosys",
    "description": "WebAssembly version of Yosys.",
    "version": "0.10.5",
    "license": "MIT",
    "contributors": [
        "Daniel Huisman <daniel@huisman.me> (https://danielhuisman.io)"
    ],
    "repository": "github:EDAcation/yosys.js",
    "keywords": [
        "yosys",
        "rtl",
        "synthesis",
        "webassembly",
        "wasm"
    ],
    "targets": {
        "node": {
            "context": "node",
            "source": "./src/node/index.ts",
            "distDir": "./dist/node",
            "isLibrary": true,
            "optimize": true
        },
        "browser": {
            "context": "browser",
            "source": "./src/browser/index.ts",
            "distDir": "./dist/browser",
            "isLibrary": true,
            "optimize": true
        }
    },
    "browser": {
        "process": "./src/empty.js",
        "fs": "./src/empty.js",
        "buffer": "./src/empty.js",
        "path": "./src/empty.js"
    },
    "files": [
        "dist",
        "LICENSE.md",
        "README.md",
        "package.json"
    ],
    "scripts": {
        "lint": "eslint",
        "clean": "rm -rf dist",
        "build": "yarn run clean && yarn run lint && parcel build && echo dist/browser dist/node | xargs -n 1 cp build/yosys.wasm",
        "dev": "yarn run clean && parcel watch"
    },
    "dependencies": {
        "@types/emscripten": "^1.39.5",
        "@types/node": "^16.11.1"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "eslint-config-google": "^0.14.0",
        "eslint-plugin-import": "^2.25.2",
        "parcel": "^2.0.0",
        "ts-node": "^10.3.0",
        "typescript": "^4.4.4"
    }
}
